{
  "expo": {
    "name": "SinCase",
    "slug": "SinCase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/SinIcon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "src/assets/SinSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/SinIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.SinCase"
    },
    "web": {
      "favicon": "src/assets/SinIcon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
