{
  "expo": {
    "name": "SinCase",
    "slug": "SinCase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/sinicon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./src/assets/sinsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sincase.app",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/sinicon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "web": {
      "favicon": "./src/assets/sinicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "e19aedf8-3663-40bc-a968-ee1df4749a44"
      }
    }
  }
}
